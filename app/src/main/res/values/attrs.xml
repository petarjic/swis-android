    <declare-styleable name="SparkCenterView">
        <attr name="empty_text" format="string" />
        <attr name="empty_drawable" format="integer" />
        <attr name="empty_textsize" format="dimension" />
        <attr name="empty_padding_top" format="dimension" />
        <attr name="empty_textcolor" format="color" />
    </declare-styleable>


</resources>
